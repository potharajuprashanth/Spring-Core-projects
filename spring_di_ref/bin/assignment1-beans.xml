<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">
      
      <bean id="product1" class="com.assignment.Product">
         
           
              <property name="manYear" value="2023"></property>
                <property name="expireYear" value="2025"></property>
                  
      
      
      
      </bean>
      
      <bean id="order1" class="com.assignment.Order">
      
        <property name="id" value="201"></property>
           <property name="orderAmount" value="999999"></property>
           <property name="product" ref="product1"></property>
           
      
      </bean>
           
 </beans>